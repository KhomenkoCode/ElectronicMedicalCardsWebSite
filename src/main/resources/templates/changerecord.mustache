	<body>
		
	<div class="fh5co-loader"></div>
	
	<div id="page">
	
	{{> header}}
 

	<div id="fh5co-about"  role="banner" style="background-image:url(images/img_bg_1.jpg);padding-top:25pt;">
		
		<div class="container">
			<div class="about-content" style="min-height:250pt;">
				
				
   					
				<div class="col-md-12 animate-box" style="background-color:white; padding:15pt;">
					<h3>Change medical record form </h3>
					<div class="form-group" style="color:red;">
							{{errormessage}}
					</div>
						
					<form method="POST" action="">
						<div class="row form-group">
							{{#admin}}{{/admin}}
							{{^admin}}
							<div class="col-md-6">
								
								{{#patient}}
								<label>Medical doctor</label><br>
								{{#record}}{{#tDoctors}}{{doctorName}} {{doctorSurname}} {{doctorPatronymic}}{{/tDoctors}}{{/record}}
								{{/patient}}
								{{#doctor}}
								<label>Patient</label><br>
								{{#record}}
								{{#tPatients}}
								{{patientName}} {{patientSurname}}
								{{/tPatients}}
								{{/record}}
								{{/doctor}}
							</div>
							<div class="col-md-6">
								{{#patient}}
									<label for="password">Doctor password</label>
								{{/patient}}
								{{#doctor}}
									<label for="password">Patient password</label>
								{{/doctor}}
								<input type="text" id="password" name="password" class="form-control" placeholder="Password">
							</div>
							{{/admin}}
						</div>
						<div class="row form-group">
							<div class="col-md-12">
								<label for="record">Record</label> 
								<textarea name="recordtext" id="recordtext" cols="30" rows="10" class="form-control" placeholder="Medical info">{{#record}}{{medicalRecord}}{{/record}}</textarea>
							</div>
						</div>
						
						<div class="form-group">
							<input type="submit" value="Change record" class="btn btn-primary">
						</div>

					</form>		
				</div>
			
			</div>
		</div>
	</div>
  
