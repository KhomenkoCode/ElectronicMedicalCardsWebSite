	<div class="fh5co-loader"></div>
	
	<div id="page">
	
	{{> header}}
 

	<div id="fh5co-about"  role="banner" style="background-image:url(images/img_bg_1.jpg);padding-top:25pt;">
		
		<div class="container">
			<div class="about-content" style="min-height:250pt;">
				<div class="alert alert-light" role="alert" style="margin:5pt; padding:5pt;text-align: center;">
					Doctor accept list
				</div>
				
				{{#doctors}}
					<div class="alert alert-light" role="alert" style="margin:5pt; padding:5pt;">
						<table style="width:100%;">
						<tr><td style="width:50%;">
						Medical doctor: {{doctorName}} {{doctorSurname}} {{doctorPatronymic}} <br> 
						License: {{licenseNumber}} <br>
						Confirmation photo link: <a target="_blank" href="{{licenseConfirmationPhotoUrl}}">Confirmation photo</a>
						</td><td style="width:50%; text-align:right;  line-height:1.5em;">
						<form action="?ln={{licenseNumber}}" method="post">
  							<button type="submit" name="Submit" value="Confirm" class="btn btn-primary" style="width:100pt;">Confirm</button><br>
  							<button type="submit" name="Submit" value="Decline" class="btn btn-primary" style="width:100pt;">Decline</button>
						</form>
						</td>
						</tr></table>
					</div>
				{{/doctors}}
				{{^doctors}}
					<div class="alert alert-light" role="alert" style="margin:5pt; padding:5pt;">
						No medical license to confirm.
					</div>
				{{/doctors}}
					
				
			</div>
		</div>
